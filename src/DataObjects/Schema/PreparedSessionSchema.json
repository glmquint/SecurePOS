{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "mean_absolute_diff_timestamps": {
            "type": "number",
            "minimum": 0
        },
        "mean_absolute_diff_amount": {
            "type": "number",
            "minimum": 0
        },
        "median_longitude_latitude": {
            "type": "array",
            "items": [
                {"type": "number"},
                {"type": "number"}
            ],
            "minItems": 2,
            "maxItems": 2,
            "not": {
                "items": {
                    "type": "number",
                    "enum": [0.0]
                }
            }
        },
        "median_target_ip": {"type": "string", "format": "ipv4", "minLength": 1},
        "median_dest_ip": {"type": "string", "format": "ipv4", "minLength": 1}
    },
    "required": [
        "mean_absolute_diff_timestamps",
        "mean_absolute_diff_amount",
        "median_longitude_latitude",
        "median_target_ip",
        "median_dest_ip"
    ]
}