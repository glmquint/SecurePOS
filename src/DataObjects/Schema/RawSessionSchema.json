{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties" : {
    "records" : {
      "type": "array",
      "items": {
          "type" : "object",
          "properties" : {
            "uuid" : {
              "type" : "string"
            },
            "location_latitude" : {
              "type" : "number"
            },
            "location_longitude" : {
              "type" : "number"
            },
            "target_ip" : {
              "type" : "string"
            },
            "dest_ip" : {
              "type" : "string"
            },
            "timestamp" : {
              "type" : "array",
                "items" : {
                    "type" : "number"
                }
            },
            "amount" : {
              "type" : "array",
                "items" : {
                    "type" : "number"
                }
            },
            "label" : {
              "type" : "string"
            }
          },
          "required" : ["uuid"]
        }
    }
  }
}